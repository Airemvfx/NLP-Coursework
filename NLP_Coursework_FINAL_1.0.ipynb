        "\n",
        "# Tokenize the test text data\n",
        "test_encodings = tokenizer(list(x_test_bert), truncation=True, padding=True)\n",
        "\n",
        "# A custom PyTorch Dataset class.\n",
        "# PyTorch (which DistilBERT operates on) requires data to be structured with this method for training.\n",
        "class EmotionDataset(torch.utils.data.Dataset):\n",
        "    def __init__(self, encodings, labels): # Cosntructor\n",
        "        self.encodings = encodings # Pass encodings\n",
        "        self.labels = labels # Pass labels\n",
        "\n",
        "    def __getitem__(self, idx): # This method gets a tokenized input for DistilBERT\n",
        "        item = {key: torch.tensor(val[idx]) for key, val in self.encodings.items()} # get index (idx) of token\n",
        "        item['labels'] = torch.tensor(self.labels[idx]) # convert idx to torch.Tensor object for BERT\n",
        "        return item # return converted item\n",
        "\n",
        "    def __len__(self): # This just checks total number of samples (to calculate epochs etc)\n",
        "        return len(self.labels)\n",
        "\n",
        "# Create instances of the EmotionDataset() for training and test data\n",
        "train_dataset = EmotionDataset(train_encodings, y_train_bert)\n",
        "test_dataset = EmotionDataset(test_encodings, y_test_bert)\n",
        "\n",
        "# Print out\n",
        "print(\"Data preparation for DistilBERT complete.\")\n",
        "print(f\"Training dataset size: {len(train_dataset)}\")\n",
